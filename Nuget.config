<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <clear />
        <add key="github" value="https://nuget.pkg.github.com/harpm/index.json" />
        <add key="Nuget" value="https://api.nuget.org/v3/index.json" />
    </packageSources>
    <packageSourceCredentials>
        <github>
            <add key="Username" value="harpm" />
            <add key="ClearTextPassword" value="${{ secrets.GITHUBTOKEN }}" />
        </github>
    </packageSourceCredentials>
</configuration>